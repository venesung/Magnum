html {width:100%;height:100%;}
body {margin:0;padding:0;background:url('../../img/bg_body.gif') repeat-y center 0;color:#666;font-size:12px;font-family:'NanumGothic', arial, dotum, 돋움;line-height:130%;}
h1, h2, h3, h4, h5, h6 {font-size:1em;}
h1, h2, h3, h4, h5, h6, div, p, dl, dt, dd, ul, ol, li, form, fieldset, blockquote, address, table, thead, tbody, tfoot, tr, td, caption {margin:0;padding:0;}
ul, ol {list-style:none;}
p, li {line-height:130%;}
em, var {font-style:normal;}
img, fieldset, input.type-image {border:0 none;}
div, p, span, li, table td {word-break:break-all;}
table caption, legend, hr {position:absolute; top:0; left:0; width:0; height:0; overflow:hidden; padding:0; visibility:hidden; font-size:0; line-height:0; display:none;}
tablec{border-collapse:collapse;}/*table-layout:fixed;*/
input {height:20px;padding:2px 0 0 4px;border:0px;background:#fdfdfd;vertical-align:middle;border-top:1px #cdcdcd solid;border-left:1px #cdcdcd solid;border-right:1px #e9e9e9 solid;border-bottom:1px #e9e9e9 solid;font:normal 12px 'NanumGothic';color:#666;}
input.nostyle {border:0 none;background:none;vertical-align:middle;}
select {font:normal 12px 'NanumGothic';color:#666;vertical-align:middle;}
textarea {padding:5px;background:#fdfdfd;border-top:1px #cdcdcd solid;border-left:1px #cdcdcd solid;border-right:1px #e9e9e9 solid;border-bottom:1px #e9e9e9 solid;resize:none;font:normal 12px 'NanumGothic';color:#666;}
strong.ngB {font-weight:normal;font-family:'NanumGothicB'}
 
.fl {float:left;}
.fr {float:right;}
.clear {clear:both;margin:0;padding:0;font-size:0;line-height:0;}
.accessibility {display:block;height:1px;width:1px;margin:-1px -1px -1px 0;padding:0;overflow:hidden;font-size:0;line-height:0;}

div.Vspace40 {clear:both;height:40px;}


/* link */
a, a:link {color:#666;text-decoration:none;}
a:visited {color:#666;}
a:active, a:hover {color:#666;text-decoration:none;}

/* Layout Selector */
#wrap {width:1024px;margin:0 auto;background:#fff;}

/* Header */
#header {position:relative;width:960px;height:120px;margin:0 auto;background:url('../../img/header/bg_header.gif') no-repeat 0 0;font-family:arial;z-index:1000;}
h1 {padding:20px 0 15px 24px;}
.gnb {position:relative;overflow:hidden;height:64px;}
.gnb ul {float:left;}
.gnb ul li {float:left;}
.gnb ul li strong {display:block;width:202px;height:64px;background:url('../../img/header/gnb_line.png') no-repeat right 0;text-align:center;}
.gnb ul li strong.last {width:175px;background:none;}
.gnb ul li strong a {display:block;height:24px;padding:8px 0 0 0;color:#fff;font-weight:bold;font-size:14px;}
.gnb .nb1 li strong a:hover,.gnb .nb1 li strong.on a {color:#fff;background:url('../../img/header/bg_nb1.gif') repeat-x left bottom;}
.gnb .nb2 li strong {width:110px;} 
.gnb .nb2 li strong a:hover,.gnb .nb2 li strong.on a {color:#fff;background:url('../../img/header/bg_nb2.gif') repeat-x left bottom;}

.gnb .snb {position:absolute;bottom:0;left:0;float:none;width:960px;height:32px;padding:0 0 0 24px;background:url('../../img/header/bg_snb.gif') repeat-x 0 0;}
.gnb .snb li {float:left;margin:0 0 0 40px;width:auto;height:32px;background:none;}
.gnb .snb li:first-child {margin:0;}
.gnb .snb li a {display:block;height:25px;padding:7px 0 0 0;background:none;color:#999;font-family:'NanumGothic';}
.gnb .snb li a:hover,.snb li.on a {background:url('../../img/header/bg_snb2.gif') no-repeat center bottom;color:#76b6c8;}

.util {position:absolute;top:31px;right:24px;}
.util li a {font-weight:normal;font-size:11px;color:#c7c7c7;}
.util li a:hover {color:#fff;}

