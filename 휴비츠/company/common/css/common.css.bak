@charset 'utf-8';

@font-face{
  font-family:'NanumGothic';
  font-style:normal;
  font-weight:normal;
  src: url('../font/nanumGothic.eot'); 
  src: local('☺'), url('http://api.mobilis.co.kr/webfonts/Nanum/NanumGothic.woff') format('woff'), url('../font/nanumGothic.ttf') format('truetype'); 
}

@font-face{
  font-family:'NanumGothicB';
  font-style:normal;
  font-weight:bold;
  src: url('../font/nanumGothicBold.eot'); 	
  src: local('☺'), url('http://api.mobilis.co.kr/webfonts/Nanum/NanumGothicBold.woff') format('woff'), url('../font/nanumGothicBold.ttf') format('truetype'); 
